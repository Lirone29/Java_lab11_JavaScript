<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.web.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.Pane?>

<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <ScrollPane fx:id="InputPanel" layoutX="32.0" layoutY="102.0" prefHeight="255.0" prefWidth="226.0">
         <content>
            <TextArea fx:id="inputTextArea" prefHeight="200.0" prefWidth="200.0" />
         </content>
        </ScrollPane>
        <ScrollPane fx:id="outputPanel" layoutX="323.0" layoutY="102.0" prefHeight="255.0" prefWidth="218.0">
         <content>
            <WebView fx:id="outputWebView" prefHeight="200.0" prefWidth="200.0" />
         </content>
        </ScrollPane>
        <ComboBox fx:id="chooseBox" layoutX="32.0" layoutY="26.0" onAction="#getChooseBoxAction" prefWidth="150.0" />
        <Label fx:id="inputLabel" layoutX="32.0" layoutY="70.0" prefHeight="17.0" prefWidth="69.0" text="Input" />
        <Label fx:id="outputLabel" layoutX="323.0" layoutY="70.0" prefHeight="17.0" prefWidth="59.0" text="Output" />
        <Button fx:id="formatButton" layoutX="327.0" layoutY="26.0" mnemonicParsing="false" onAction="#formatHandler" prefHeight="25.0" prefWidth="59.0" text="Format" />
    </children>
</Pane>
